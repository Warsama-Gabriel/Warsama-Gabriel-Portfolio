
<h2><%= link_to "#{portfolio_item.title}", portfolio_show_path(portfolio_item) %></h2>
<p><%= portfolio_item.subtitle %></p>
<p><%= portfolio_item.body %></p>
<p><%= image_tag(portfolio_item.thumb_image)%></p>
<p><%= link_to 'edit_portfolio item', edit_portfolio_path(portfolio_item) if logged_in?(:siteamin) %></p>
